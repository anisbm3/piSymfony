<form method="post" action="{{ path('app_cosplay_delete', {'id': cosplay.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ cosplay.id) }}">
    <button type="submit" class="btn ">
        <i class="far fa-trash-alt"></i> Supprimer
    </button>
</form>
