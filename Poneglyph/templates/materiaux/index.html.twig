{% extends 'base-back.html.twig' %}

{% block title %}Materiaux index{% endblock %}

{% block body %}
    <h1>Materiaux index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nomma</th>
                <th>Typema</th>
                <th>Disponibilite</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for materiaux in materiauxes %}
            <tr>
                <td>{{ materiaux.id }}</td>
                <td>{{ materiaux.nomma }}</td>
                <td>{{ materiaux.typema }}</td>
                <td>{{ materiaux.disponibilite }}</td>
                <td>
                    <a href="{{ path('app_materiaux_show', {'id': materiaux.id}) }}">show</a>
                    <a href="{{ path('app_materiaux_edit', {'id': materiaux.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_materiaux_new') }}">Create new</a>
{% endblock %}
